<resources>

    <declare-styleable name="ButtonSettingsToggle">
        <attr name="enable" format="boolean" />
        <attr name="title" format="string"/>
        <attr name="subtitle" format="string"/>
        <attr name="itemText" format="string"/>
        <attr name="buttonType" format="enum">
            <enum name="Switch" value="0"/>
            <enum name="TextItem" value="1"/>
        </attr>
    </declare-styleable>

</resources>